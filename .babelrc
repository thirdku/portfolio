{
  "env": {
    "test": {
      "presets": [
        ["preact-cli/babel", { "modules": "commonjs" }]
      ]
    }
  }
  {
  "plugins": [
        ["module-resolver", {
        "root": ["."],
        "alias": {
            "react": "preact/compat",
            "react-dom": "preact/compat"
        }
        }]
    ],
    "presets": [
        "react"
    ]
    "plugins": [
    ["@babel/plugin-transform-react-jsx", {
      "pragma": "h",
      "pragmaFrag": "Fragment",
    }]
  ]
}
}
